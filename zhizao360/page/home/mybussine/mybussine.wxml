<import src="../common/homeCommon.wxml"></import>
<view class="view-body">
  <view class="swiper-box">
    <swiper bindchange="swiperImg" indicator-dots="{{indicatorDots}}" vertical="{{vertical}}" autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}">
      <block wx:key="*this" wx:for="{{pictrue[pictrueIndex]}}">
        <swiper-item>
          <view class="swiper-item">
            <image class="bannerImage" src="{{item}}"></image>
          </view>
        </swiper-item>
      </block>
    </swiper>
    <view class="pictrueSumBox">
      <text>{{currentNum}}/{{totalNum}}</text>
    </view>
    <view class="swiperNav">
      <view class="swiperNav-itemBox">
        <scroll-view class="scroll-view_H" scroll-x="true" style="width: 100%">
          <block wx:key="*this" wx:for="{{swiperNavArray}}">
            <view class="swiperNav-item {{index == swiperNavArray.length-1 ? '':'bd-r1px' }} {{index == pictrueIndex ? 'navShow':'' }}" bindtap="switchNav" data-index="{{index}}">{{item}}</view>
          </block>
        </scroll-view>
      </view>
    </view>
  </view>
